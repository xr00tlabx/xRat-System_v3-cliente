{
"name": "xrat-servidor-v3",
"version": "3.0.0",
"description": "Servidor Modular WebSocket/HTTP para xRat System v3 - Cliente",
"main": "src/server.js",
  "scripts": {
    "setup": "node setup.js",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node test-client.js",
    "test:http": "node test-http.js",
    "logs": "tail -f logs/xrat-server.log",
    "logs:error": "tail -f logs/xrat-error.log",
    "clean": "rm -rf logs/*.log",
    "clean:win": "del /Q logs\\*.log",
    "install:deps": "npm install",
    "health": "curl http://localhost:3333/api/system/health",
    "status": "curl http://localhost:3333/api/system/status"
  },
  "keywords": [
    "websocket",
    "xrat",
    "socket",
    "servidor",
    "express",
    "modular",
    "http",
    "api"
  ],
  "author": "Desenvolvedor C++",
  "license": "ISC",
  "dependencies": {
    "ws": "^8.14.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
},
"repository": {
    "type": "git",
    "url": "https://github.com/xr00tlabx/xRat-System_v3-cliente"
},
"directories": {
    "src": "./src",
    "logs": "./logs"
  }
}
